    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_ajto" type="st_cimelem_ajto">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének ajtó része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s door number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_cim_id" type="st_cimelem_cim_id">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének OECM CímID-ja.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s address identifier</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_email" type="st_email">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett elektronikus címe</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s email address</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_emelet" type="st_cimelem_emelet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének emelet része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s floor number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_epulet" type="st_cimelem_epulet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének épület része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s building number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_hazszam" type="st_cimelem_hazszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének házszám része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s street number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_hely" type="st_cimelem_nemz_hely">
    <xs:annotation>
      <xs:documentation xml:lang="hu">
        Címzett helység neve.
        Település neve, pl.: London.
      </xs:documentation>
      <xs:documentation xml:lang="en">
        Addressee's name of settlement.
        Settlement name, e.g.: London.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_irsz" type="st_cimelem_irszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének irányítószáma.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee's postcode</xs:documentation>
      <xs:documentation xml:lang="hu">Irányítószám (területi alapú díjazás esetén kötelező)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_kozelebbi_cim" type="st_cimelem_kozelebbi_cim">
    <xs:annotation>
      <xs:documentation xml:lang="hu">
        Címzett közelebbi címe.
        pl. 10 Downing Street.
      </xs:documentation>
      <xs:documentation xml:lang="en">
        Addressee's detailed address.
        Either the entire detailed address (e.g. Kossuth tér 2) or the parts of the address should be entered.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_kozterulet_jelleg" type="st_cimelem_kozterulet_jelleg">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett címének közterület jellege része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee street type</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_kozterulet_nev" type="st_cimelem_kozterulet_nev">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett közterület név.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee street name</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_lepcsohaz" type="st_cimelem_lepcsohaz">
