      <xs:documentation xml:lang="hu">Címzett címének lépcsőház része.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s stairway number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_nev" type="st_cimelem_nev">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett neve/megnevezése.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s name</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_postafiok" type="st_cimelem_postafiok">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett postafiók száma.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s post office box number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="cimzett_telefon" type="st_telefonszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Címzett telefonszáma.</xs:documentation>
      <xs:documentation xml:lang="en">Addressee’s phone number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="dij" type="st_dij">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladási díj.</xs:documentation>
      <xs:documentation xml:lang="en">Dispatch charge</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_adatok" type="ct_efj_adatok">
    <xs:annotation>
      <xs:documentation xml:lang="hu">EFJ feladó technikai adatok</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_gyartokod" type="st_efj_gyartokod">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Pl. nyomdák kódja, ha több megállapodásra is szállít.</xs:documentation>
      <xs:documentation xml:lang="en">For example, codes of printing companies, if several agreements are covered.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_pre_id" type="st_efj_pre_id">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Előrejelzéses állomány beküldésekor visszakapott IKR-es EFJ-azonosító.</xs:documentation>
      <xs:documentation xml:lang="en">It should be filled in if the value of efj_tipus is „F”, and its value should be the EFJ identifier of IKR.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_szoftver" type="st_efj_szoftver">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az elektronikus feladójegyzéket készítő neve, a szoftver neve és verziószáma.</xs:documentation>
      <xs:documentation xml:lang="en">Name of the person creating the electronic posting list, name and version of the software.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_tipus" type="st_efj_tipus" default="F">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladás vagy előrejelzés típusú jegyzék beazonosítására szolgáló mező.</xs:documentation>
      <xs:documentation xml:lang="en">Field used for identifying a despatch (F) or advance notice (E) type list.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="efj_zaras" type="st_shortdatetime">
    <xs:annotation>
      <xs:documentation xml:lang="hu">EFJ zárás dátuma és időpontja</xs:documentation>
      <xs:documentation xml:lang="en">Date and time of closing the EFJ</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="engedelyszam" type="st_engedelyszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">
        A posta által megadott Bevizsgálási engedély száma.
        Több azonosító esetén vesszővel kell elválasztani egymástól.
      </xs:documentation>
      <xs:documentation xml:lang="en">Number of the Testing licence provided by Magyar Posta</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="eny_osszeg" type="st_eny_osszeg">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Értéknyilvánítás összege.</xs:documentation>
      <xs:documentation xml:lang="en">Value insurance amount</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ertesites" type="ct_ertesites">
    <xs:annotation>
      <xs:documentation>Értesítési információk</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_ajto" type="st_cimelem_ajto">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének ajtószám része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s door number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_cim_id" type="st_cimelem_cim_id">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének OECM CímID-ja.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s address identifier</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_email" type="st_email">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó email címe.</xs:documentation>
      <xs:documentation xml:lang="en">The sender’s email address</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_emelet" type="st_cimelem_emelet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének emelet része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s floor</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_epulet" type="st_cimelem_epulet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének épület része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s building (e.g. Building "B")</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_hazszam" type="st_cimelem_hazszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének házszám része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s street number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_hely" type="st_cimelem_hely">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének helység része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender's settlement name</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_irsz" type="st_cimelem_irszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének postai irányítószám része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s postcode</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_kozelebbi_cim" type="st_cimelem_kozelebbi_cim">
    <xs:annotation>
      <xs:documentation xml:lang="hu">
        Feladó közelebbi címe.
        pl. 10 Downing Street.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_kozterulet_jelleg" type="st_cimelem_kozterulet_jelleg">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének közterület jellege része.</xs:documentation>
      <xs:documentation xml:lang="en">Street type (e.g. street, road, square)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_kozterulet_nev" type="st_cimelem_kozterulet_nev">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének közterület név vagy helyrajzi szám része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s street name (e.g. Petőfi)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_lepcsohaz" type="st_cimelem_lepcsohaz">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó címének lépcsőház része.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s stairway (e.g. Stairway “B”)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_megallapodas" type="st_felado_megallapodas">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A szerződésben szereplő megállapodásszám.</xs:documentation>
      <xs:documentation xml:lang="en">Agreement number appearing in the contract</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_megjegyzes" type="st_felado_megjegyzes">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó szabadon töltheti, pl.: a feladási adatokat tartalmazó file neve.</xs:documentation>
      <xs:documentation xml:lang="en">The sender may enter anything, e.g. the file name of the file containing posting data</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_nev" type="st_cimelem_nev">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Természetes, vagy jogi személy elnevezése, akivel szerződéses jogviszonyban áll a Posta.</xs:documentation>
      <xs:documentation xml:lang="en">Name of an individual or a legal entity with whom/which Posta concluded a contract.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_postafiok" type="st_cimelem_postafiok">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó postafiók száma.</xs:documentation>
      <xs:documentation xml:lang="en">Sender’s post office box number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_telefon" type="st_telefonszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó telefonszáma.</xs:documentation>
      <xs:documentation xml:lang="en">The sender's phone number</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felado_vevokod" type="st_felado_vevokod">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladó (ügyfél) azonosító.</xs:documentation>
      <xs:documentation xml:lang="en">Sender (customer) identification</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="gepre_alkalmassag" type="st_gepre_alkalmassag">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Gépi feldolgozásra alkalmas küldemények jelölése.</xs:documentation>
      <xs:documentation xml:lang="en">Indication of mail items suitable for automated processing.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="hiv_ertesito">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Értesítő száma. Kizárólag hivatalos iratnál, a címzett távolléte esetén otthagyandó értesítés száma/betűjele. Kézbesítéshez szükséges információ, ügyfélnek visszaadni nem kell.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="st_hiv_ertesito"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="hiv_irat_fajta" type="st_hiv_irat_fajta">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Hivatalos irat fajtája.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="hiv_iratszam" type="st_hiv_iratszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Hivatalos irat száma.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="idopont" type="st_shortdatetime">
    <xs:annotation>
      <xs:documentation>időpont</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="kezb_mod" type="st_kezb_mod" default="HA">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Kézbesítési mód.</xs:documentation>
      <xs:documentation xml:lang="en">Delivery method</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="kezbesites" type="ct_kezbesites">
    <xs:annotation>
      <xs:documentation>Kézbesítési információ</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="kezdoallas" type="st_bermentesito_allas">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Bérmentesítő-gép kezdő állása.</xs:documentation>
      <xs:documentation xml:lang="en">Start position of franking machine</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="kezelesi_mod" type="st_kezelesi_mod" default="2">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Értéknyilvánítás esetén a kezelési mód.</xs:documentation>
      <xs:documentation xml:lang="en">Method of treatment</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="konszolidator_feladas" type="st_konszolidator_feladas" default="N">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Konszolidátoros feladás jelzése.</xs:documentation>
      <xs:documentation xml:lang="en">Despatch with consolidator</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="kulonszolgok" type="st_kulonszolgok">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Többletszolgáltatások.</xs:documentation>
      <xs:documentation xml:lang="en">Additional services</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="meret" type="st_meret" default="1">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Küldeményméret jelzése.</xs:documentation>
      <xs:documentation xml:lang="en">Indication of mail item size</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="sajat_azonosito" type="st_sajat_azonosito">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Saját azonosító, megjegyzés.</xs:documentation>
      <xs:documentation xml:lang="en">Own identifier, comment</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="sorszam" type="st_sorszam">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Feladási tétel sorszáma.</xs:documentation>
      <xs:documentation xml:lang="en">Dispatch item number, strictly incremented by one (starting with 1) for each dispatch entry</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="statusz" type="st_tetel_statusz">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A küldemény állapota a jegyzék zárásakor</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="felvetel_datum" type="st_shortdate">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A küldemény felvételi dátuma</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="suly">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A küldeményfajtára vonatkozó mindenkori díjszabás szerinti súlytartományok felső határa grammban.</xs:documentation>
      <xs:documentation xml:lang="en">Upper weight limit of the item type's weight ranges according to the tariff, in grams.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="st_suly">
        <xs:minLength value="1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="teruleti_tarifa" type="st_teruleti_tarifa">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Területi tarifa.</xs:documentation>
      <xs:documentation xml:lang="en">Area-based tariff</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="tv_cimzett_ajto" type="st_cimelem_ajto">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Tértivevény visszaküldés cím címének ajtó része.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="tv_cimzett_cim_id" type="st_cimelem_cim_id">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Tértivevény visszaküldés cím címének OECM CímID-ja.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="tv_cimzett_emelet" type="st_cimelem_emelet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Tértivevény visszaküldés cím címének emelet része.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="tv_cimzett_epulet" type="st_cimelem_epulet">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Tértivevény visszaküldés cím címének épület része.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="tv_cimzett_hazszam" type="st_cimelem_hazszam">
